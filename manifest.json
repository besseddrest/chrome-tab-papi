{
    "manifest_version": 3,
    "name": "Tab Daddy",
    "version": "0.1.0",
    "description": "Blazingly fast tab management via your keyboard",
    "action": {
        "default_popup": "index.html",
        "default_icon": "images/papi_128.png"
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}
