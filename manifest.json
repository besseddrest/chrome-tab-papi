{
    "manifest_version": 3,
    "name": "Tab Papi",
    "version": "1.0",
    "description": "Manage your tab clutter with speed from your keyboard.",
    "action": {
        "default_popup": "papi.html",
        "default_icon": "images/papi_128.png"
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}
